{
    "name": "patch-dot-cab",
    "description": "Modular Eurorack style synthesizer made with Web Audio",
    "version": "1.0.1",
    "license": "MIT",
    "scripts": {
        "start": "patchcab",
        "build": "patchcab --build --rack patchcab.json"
    },
    "dependencies": {
        "@banksean/patchcab-cvsequencer": "^0.0.1",
        "@banksean/patchcab-midiin": "^0.0.1",
        "@banksean/patchcab-polyrhythm": "^0.0.1",
        "@patchcab/core": "1.0.5",
        "@patchcab/modules": "1.0.5",
        "apisauce": "^2.0.1",
        "patchcab-kaleido": "^1.0.2"
    },
    "devDependencies": {
        "@types/node": "^14.14.22",
        "@vercel/node": "^1.9.0"
    },
    "prettier": {
        "useTabs": false,
        "arrowParens": "always",
        "semi": true,
        "bracketSpacing": true,
        "singleQuote": true,
        "printWidth": 90
    }
}